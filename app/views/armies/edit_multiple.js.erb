if ('<%= @action %>' == 'update') {
  $('#content').html("<%= j render 'form_update_multiple', armies: @armies, action: @action %>");
}

if ('<%= @action %>' == 'destroy') {
  $('#content').html("<%= j render 'form_destroy_multiple', armies: @armies, action: @action %>");
}

if ('<%= @action %>' == 'import') {
  $('#content').html("<%= j render 'form_import', action: @action %>");
}

$('#armyModal').modal('show');
